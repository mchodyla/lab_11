<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.E3_controller">
   <children>
      <TabPane fx:id="tabPane">
         <tabs>
            <Tab fx:id="tabRent" closable="false" text="Wypożyczalnia">
               <content>
                  <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="312.0" prefWidth="600.0" spacing="5.0">
                     <children>
                        <HBox alignment="CENTER" spacing="5.0">
                           <children>
                              <RadioButton fx:id="radioButtonRenting" mnemonicParsing="false" selected="true" text="Wypożyczanie">
                                 <toggleGroup>
                                    <ToggleGroup fx:id="toggleGroup" />
                                 </toggleGroup>
                              </RadioButton>
                              <RadioButton fx:id="radioButtonReturning" mnemonicParsing="false" text="Oddawanie" toggleGroup="$toggleGroup" />
                              <Region HBox.hgrow="ALWAYS" />
                              <ComboBox fx:id="comboBox" prefWidth="250.0" promptText="*Wypożyczający" HBox.hgrow="ALWAYS" />
                           </children>
                           <padding>
                              <Insets left="7.0" right="5.0" top="5.0" />
                           </padding>
                        </HBox>
                        <TextField fx:id="searchField" promptText="Wyszukiwanie">
                           <VBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </VBox.margin>
                        </TextField>
                        <HBox alignment="CENTER" prefHeight="150.0" prefWidth="590.0" spacing="3.0" VBox.vgrow="ALWAYS">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                           <children>
                              <ListView fx:id="leftListView" HBox.hgrow="ALWAYS" />
                              <ListView fx:id="rightListView" HBox.hgrow="ALWAYS" />
                           </children>
                           <padding>
                              <Insets left="5.0" right="5.0" />
                           </padding>
                        </HBox>
                        <TableView fx:id="descriptionTable" prefHeight="55.0" prefWidth="600.0">
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                           <VBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </VBox.margin>
                        </TableView>
                     </children>
                  </VBox>
               </content>
            </Tab>
            <Tab fx:id="tabHistory" closable="false" text="Historia">
               <content>
                  <VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="312.0" prefWidth="600.0" spacing="5.0">
                     <children>
                        <TableView fx:id="historyTable" prefWidth="200.0">
                           <VBox.margin>
                              <Insets left="5.0" right="5.0" top="5.0" />
                           </VBox.margin>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                     </children>
                  </VBox>
               </content>
            </Tab>
            <Tab fx:id="tabAdd" closable="false" text="Dodawanie">
               <content>
                  <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="312.0" prefWidth="600.0" spacing="5.0">
                     <children>
                        <Region VBox.vgrow="ALWAYS" />
                        <VBox alignment="CENTER">
                           <children>
                              <HBox alignment="CENTER" spacing="5.0">
                                 <children>
                                    <Label text="Nazwa narzędzia*" />
                                    <Region HBox.hgrow="ALWAYS" />
                                    <TextField fx:id="toolNameField" prefWidth="300.0" promptText="(max 100 znaków)" />
                                 </children>
                                 <padding>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER" spacing="5.0">
                                 <children>
                                    <Label text="Lokalizacja*" />
                                    <Region HBox.hgrow="ALWAYS" />
                                    <ComboBox fx:id="locationBox" prefWidth="300.0" promptText="Pomieszczenie + miejsce" />
                                 </children>
                                 <padding>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER" spacing="5.0">
                                 <children>
                                    <Label text="Data zakupu" />
                                    <Region HBox.hgrow="ALWAYS" />
                                    <DatePicker fx:id="datePicker" prefWidth="300.0" />
                                 </children>
                                 <padding>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER" spacing="5.0">
                                 <children>
                                    <Label text="Cena" />
                                    <Region HBox.hgrow="ALWAYS" />
                                    <TextField fx:id="toolPriceField" prefWidth="300.0" promptText="(max 99,999,999.99)" />
                                 </children>
                                 <padding>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </padding>
                              </HBox>
                           </children>
                           <padding>
                              <Insets left="20.0" right="20.0" />
                           </padding>
                        </VBox>
                        <Region VBox.vgrow="ALWAYS" />
                        <Label text="* - wymagane pola" />
                     </children>
                     <padding>
                        <Insets top="5.0" />
                     </padding>
                  </VBox>
               </content>
            </Tab>
         </tabs>
      </TabPane>
      <VBox VBox.vgrow="NEVER">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <children>
            <HBox alignment="CENTER">
               <children>
                  <Button fx:id="b_cancel" mnemonicParsing="false" onAction="#gotoMenu" prefWidth="100.0" text="Wstecz" />
                  <Region HBox.hgrow="ALWAYS" />
                  <HBox>
                     <children>
                        <Button fx:id="b_rent" disable="true" mnemonicParsing="false" onAction="#rentTools" prefWidth="100.0" text="Wypożycz">
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="b_return" disable="true" mnemonicParsing="false" onAction="#rightButtonMethod" prefWidth="100.0" text="Oddaj">
                           <HBox.margin>
                              <Insets left="5.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <Separator />
            <HBox alignment="CENTER">
               <children>
                  <Label fx:id="statusLabel" text="Status" />
                  <Region HBox.hgrow="ALWAYS" />
                  <Label fx:id="statusLabel2" text="Status" visible="false" />
               </children>
               <padding>
                  <Insets left="2.0" right="2.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</VBox>
