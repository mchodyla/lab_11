<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="5.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.E3_controller">
   <children>
      <HBox alignment="CENTER" spacing="5.0" VBox.vgrow="NEVER">
         <children>
            <RadioButton fx:id="radioButtonRenting" mnemonicParsing="false" selected="true" text="Wypożyczanie">
               <toggleGroup>
                  <ToggleGroup fx:id="toggleGroup" />
               </toggleGroup></RadioButton>
            <RadioButton fx:id="radioButtonReturning" mnemonicParsing="false" text="Oddawanie" toggleGroup="$toggleGroup" />
            <ComboBox fx:id="comboBox" prefWidth="250.0" promptText="Wypożyczający" HBox.hgrow="ALWAYS" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
      <TextField fx:id="searchField" promptText="Wyszukiwanie">
         <VBox.margin>
            <Insets left="5.0" right="5.0" />
         </VBox.margin>
      </TextField>
      <HBox alignment="CENTER" prefHeight="150.0" prefWidth="590.0" spacing="5.0" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" />
         </VBox.margin>
         <children>
            <ListView fx:id="leftListView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
            <VBox alignment="CENTER" spacing="5.0" HBox.hgrow="ALWAYS">
               <children>
                  <ListView fx:id="rightListView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
               </children>
            </VBox>
         </children>
      </HBox>
      <TableView fx:id="descriptionTable" prefHeight="75.0" prefWidth="600.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox alignment="CENTER" spacing="250.0" VBox.vgrow="NEVER">
         <children>
            <Button fx:id="b_cancel" mnemonicParsing="false" onAction="#gotoMenu" prefWidth="100.0" text="Anuluj" />
            <HBox>
               <children>
                  <Button fx:id="b_rent" disable="true" mnemonicParsing="false" onAction="#rentATool" prefWidth="100.0" text="Wypożycz">
                     <HBox.margin>
                        <Insets right="5.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="b_return" disable="true" mnemonicParsing="false" onAction="#returnTools" prefWidth="100.0" text="Oddaj">
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </HBox>
      <Separator prefWidth="200.0" />
      <AnchorPane prefHeight="15.0" VBox.vgrow="NEVER">
         <children>
            <Label fx:id="statusLabel" text="Status" />
         </children>
      </AnchorPane>
   </children>
</VBox>
