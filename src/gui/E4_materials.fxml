<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.E4_controller">
   <children>
      <TabPane fx:id="tabPane">
         <tabs>
            <Tab fx:id="tabWarehouse" closable="false" text="Magazyn">
               <content>
                  <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="600.0" spacing="5.0">
                     <children>
                        <TextField fx:id="searchField" promptText="Wyszukiwanie">
                           <VBox.margin>
                              <Insets left="5.0" right="5.0" top="7.0" />
                           </VBox.margin>
                        </TextField>
                        <HBox alignment="CENTER" prefHeight="185.0" spacing="5.0" VBox.vgrow="NEVER">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                           <children>
                              <ListView fx:id="listView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                           </children>
                           <padding>
                              <Insets left="5.0" right="5.0" />
                           </padding>
                        </HBox>
                        <TableView fx:id="tableView" fixedCellSize="0.0" prefHeight="55.0">
                           <VBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </VBox.margin>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                     </children>
                  </VBox>
               </content>
            </Tab>
            <Tab fx:id="tabAdd" closable="false" text="Dodawanie">
              <content>
                  <VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="312.0" prefWidth="600.0" spacing="5.0">
                     <children>
                        <VBox VBox.vgrow="ALWAYS">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                           <children>
                              <Region VBox.vgrow="ALWAYS" />
                              <VBox alignment="TOP_CENTER">
                                 <children>
                                    <HBox alignment="CENTER">
                                       <children>
                                          <Label text="Nazwa*" />
                                          <Region HBox.hgrow="ALWAYS" />
                                          <TextField fx:id="nameField" prefWidth="300.0" promptText="(max 100 znaków)" />
                                       </children>
                                       <padding>
                                          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                       </padding>
                                    </HBox>
                                    <HBox alignment="CENTER">
                                       <children>
                                          <Label text="Lokalizacja*" />
                                          <Region HBox.hgrow="ALWAYS" />
                                          <ComboBox fx:id="locationBox" prefWidth="300.0" promptText="Pomieszczenie + miejsce" />
                                       </children>
                                       <padding>
                                          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                       </padding>
                                    </HBox>
                                    <HBox alignment="CENTER">
                                       <children>
                                          <Label text="Ilość jednostkowa" />
                                          <Region HBox.hgrow="ALWAYS" />
                                          <TextField fx:id="amountField" disable="true" prefWidth="300.0" promptText="(max 100 znaków)" />
                                       </children>
                                       <padding>
                                          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                       </padding>
                                    </HBox>
                                    <HBox alignment="CENTER">
                                       <children>
                                          <Label text="Data zakupu" />
                                          <Region HBox.hgrow="ALWAYS" />
                                          <DatePicker disable="true" prefWidth="300.0" />
                                       </children>
                                       <padding>
                                          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                       </padding>
                                    </HBox>
                                    <HBox alignment="CENTER">
                                       <children>
                                          <Label text="Cena" />
                                          <Region HBox.hgrow="ALWAYS" />
                                          <TextField fx:id="searchField14" disable="true" prefWidth="300.0" promptText="(max 99,999,999.99)" />
                                       </children>
                                       <padding>
                                          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                       </padding>
                                    </HBox>
                                 </children>
                                 <padding>
                                    <Insets left="20.0" right="20.0" />
                                 </padding>
                              </VBox>
                              <Region VBox.vgrow="ALWAYS" />
                           </children>
                        </VBox>
                        <Label text="* - wymagane pola" />
                     </children>
                  </VBox>
              </content>
            </Tab>
         </tabs>
      </TabPane>
      <VBox>
         <children>
            <HBox alignment="BASELINE_LEFT" spacing="5.0">
               <children>
                  <Button fx:id="b_cancel" mnemonicParsing="false" onAction="#gotoMenu" prefWidth="100.0" text="Wstecz">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
                  <Region HBox.hgrow="ALWAYS" />
                  <ComboBox fx:id="comboBox" promptText="Typ zasobu" HBox.hgrow="ALWAYS" />
                  <Region HBox.hgrow="ALWAYS" />
                  <Button fx:id="b_add" mnemonicParsing="false" onAction="#gotoMenu" prefWidth="100.0" text="Dodaj" visible="false" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <Separator prefWidth="200.0" />
            <HBox VBox.vgrow="NEVER">
               <children>
                  <Label fx:id="statusLabel" text="Status" />
                  <Region HBox.hgrow="ALWAYS" />
                  <Label fx:id="statusLabel2" text="Status" visible="false" />
               </children>
               <padding>
                  <Insets left="2.0" right="2.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</VBox>
